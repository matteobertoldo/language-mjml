
'.text.mjml.basic, .text.mjml, .text.mjml .meta.scope.between-tag-pair, .text.mjml .punctuation.tag.begin':

# MJML elements

  'Mjml Divider':
    'prefix': 'divider'
    'body': """
      <mj-divider $1 />
    """

  'Mjml Invoice':
    'prefix': 'invoice'
    'body': """
      <mj-invoice>\n\t$1\n</mj-invoice>
    """

