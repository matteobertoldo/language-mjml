# MJML Template
  'Basic MJML Template':
    'prefix': 'mjml-'
    'body': """
      <mjml>
      \t<mj-head>
      \t\t<mj-title>$1</mj-title>
      \t\t<mj-attributes>
      \t\t\t$2
      \t\t</mj-attributes>
      \t</mj-head>
      \t<mj-body>
      \t\t<mj-section>
      \t\t\t<mj-column>
      \t\t\t\t\t$3
      \t\t\t</mj-column>
      \t\t</mj-section>
      \t</mj-body>
      </mjml>
    """
